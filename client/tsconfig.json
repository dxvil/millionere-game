{
    "compilerOptions": {
      "moduleResolution": "node",
      "outDir": "./dist/",
      "noImplicitAny": false,
      "module": "es6",
      "lib": ["es6", "dom", "es2016", "es2017"],
      "target": "es2016",
      "jsx": "react",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true
    },
    "include": [
      "src/**/*"
, "index.tsx"    ],
    "exclude": ["node_modules", "dist"]
  }
  